@{
    ViewBag.Title = "Upload a Cert";
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Cert Page</title>
</head>
<body>
    @using (Html.BeginForm())
    {
        <div class="form-horizontal">

            <h1>Upload your cert</h1>
            @*       <form action="" method="post" enctype="multipart/form-data">*@
            <div class="form-group">
                
                    <div class="col-md-10">
                        <label for="file" class="control-label col-md-2">Filename</label>
                        <input type="file" name="file" id="file" class="form-control" />
                    
                    </div>
            </div>
            <div class="form-group">
                <div class="col-md-10">
                    <label for="pwd" class="control-label col-md-2">Password</label>
                    <input type="text" id="pwd" name="pwd" class="form-control" />
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-10">
                    <label for="keyName" class="control-label col-md-2">Key Name</label>
                    <input type="text" id="Key Name" name="keyName" class="form-control" />
                </div>
            </div>

            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" class="btn btn-default" value="Upload" />
                </div>
            </div>
        </div>
            @* </form>*@

    }
</body>
</html>
